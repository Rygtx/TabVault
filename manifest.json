{
  "manifest_version": 3,
  "name": "TabVault",
  "version": "1.0",
  "description": "保存和恢复浏览器标签页状态的简单 Chrome 扩展",
  "permissions": [
    "storage",
    "tabs",
    "alarms",
    "downloads",
    "unlimitedStorage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}